<odoo>
    <record id="view_dms_version_tree" model="ir.ui.view">
        <field name="name">dms_version.tree</field>
        <field name="model">dms.version</field>
        <field name="arch" type="xml">
            <tree default_order="create_date desc">
                <field name="create_date" />
                <field name="name" string="Filename" invisible="1" />
                <field name="content" widget="binary" filename="name" />
                <field name="size" widget="binary_size" string="Size" />
            </tree>
        </field>
    </record>
</odoo>
