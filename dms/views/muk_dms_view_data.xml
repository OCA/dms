<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="muk_dms_database_data_tree" model="ir.ui.view">
		<field name="name">muk_dms_database_data.tree</field>
		<field name="model">muk_dms.database_data</field>
		<field name="arch" type="xml">
			<tree string="Files">
				<field name="id" />
				<field name="file" />
			</tree>
		</field>
	</record>
	<record id="action_muk_dms_database_data" model="ir.actions.act_window">
		<field name="name">Database Files</field>
		<field name="res_model">muk_dms.database_data</field>
		<field name="view_mode">tree</field>
	</record>
	<menuitem id="menu_muk_dms_database_data" name="Database Files"
	parent="cat_menu_muk_dms_files" action="action_muk_dms_database_data" />
</odoo>